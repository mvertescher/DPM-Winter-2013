package states;

import robot.Robot;

/**
 * Localization.java
 * 
 * Initial localization state 
 * 
 * @author Matthew Vertescher
 *
 */
public class Localizing implements State {

	private StateID id; 
	private Robot robot;
	
	
	public Localizing(Robot r) {
		this.robot = r;
		this.id = StateID.LOCALIZING;
	}
	
	
	public void ultrasonicLocalization() {
		
	}
	
	public void lightLocalization() {
		
	}


	@Override
	public StateID getStateID() {
		return id;
	}
	
	
}
