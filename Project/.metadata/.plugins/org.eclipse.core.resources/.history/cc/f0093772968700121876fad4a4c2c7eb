package experimental;

import robot.Robot;
import lejos.geom.Point;
import lejos.robotics.subsumption.Arbitrator;
import lejos.robotics.subsumption.Behavior;

public class Attacker extends CricketBot {
	
	public Attacker(Robot r) {
		super(r); 
		Behavior[] b = new Behavior[2];
		b[1] = new Move(this, new Point(20,0));
		b[0] = new Avoid(this); 
		this.setArbitrator(new Arbitrator(b,true));
		
		r.startUltrasonicTimers();
	}
	
}
