package experimental;

import lejos.nxt.Sound;
import lejos.robotics.subsumption.Behavior;

public class Shutdown implements Behavior {

	public Shutdown
	
	@Override
	public boolean takeControl() {
		return true;
	}

	@Override
	public void action() {
		Sound.beepSequenceUp();
		snooze(2000);
	}

	@Override
	public void suppress() {
		// Dead code
	}

}
