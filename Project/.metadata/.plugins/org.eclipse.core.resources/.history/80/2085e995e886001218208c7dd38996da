package states;

public class StateController {
	
	private State[] states;
	
	public StateController() {
		states = new State[3];
		states[StateID.LOCALIZING.ordinal()] = new Localizing(this); 
		states[StateID.TRAVELLING.ordinal()] = new Travelling(this); 
		states[StateID.AVOIDING.ordinal()] = new Avoiding(this); 
	}
	
	
}
